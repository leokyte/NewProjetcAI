{
	"name": "kyte",
	"version": "2.4.0",
	"private": true,
	"scripts": {
		"android": "npx react-native run-android --mode posDebug --main-activity com.kyte.MainActivity",
		"ios": "npx react-native run-ios",
		"start": "npx react-native start --reset-cache && ENVFILE=.env.stage",
		"test": "jest",
		"postinstall": "patch-package",
		"build:ios:jsbundle": "npx react-native bundle --entry-file='index.js' --bundle-output='./ios/kyte/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
		"build:android:jsbundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
		"build:android:jsbundle:legacy": "yarn run build:android:jsbundle",
		"gradlew:clean": "cd android && ./gradlew clean",
		"gradlew:bundle:catalog": "cd android && ./gradlew bundleCatalogRelease",
		"gradlew:bundle:pos": "cd android && ./gradlew bundlePosRelease",
		"gradlew:bundle:pos:legacy": "cd android && ./gradlew bundlePosRelease",
		"gradlew:apk:catalog": "cd android && ./gradlew assembleCatalogRelease",
		"gradlew:apk:pos": "cd android && ./gradlew assemblePosRelease",
		"build:pos:bundle:prod": "yarn run gradlew:clean && ENVFILE=.env.prod yarn run gradlew:bundle:pos --parallel",
		"build:pos:bundle:prod:legacy": "yarn run gradlew:clean && ENVFILE=.env.prod yarn run gradlew:bundle:pos --parallel",
		"build:pos:bundle:stage": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run gradlew:bundle:pos --parallel",
		"build:pos:bundle:dev": "yarn run gradlew:clean && ENVFILE=.env yarn run gradlew:bundle:pos --parallel",
		"build:pos:apk:prod": "yarn run gradlew:clean && ENVFILE=.env.prod yarn run gradlew:apk:pos --parallel",
		"build:pos:apk:stage": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run gradlew:apk:pos --parallel",
		"build:pos:apk:dev": "yarn run gradlew:clean && ENVFILE=.env yarn run gradlew:apk:pos --parallel",
		"build:catalog:bundle:prod": "yarn run gradlew:clean && ENVFILE=.env.prod yarn run gradlew:bundle:catalog --parallel",
		"build:catalog:bundle:stage": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run gradlew:bundle:catalog --parallel",
		"build:catalog:bundle:dev": "yarn run gradlew:clean && ENVFILE=.env yarn run gradlew:bundle:catalog --parallel",
		"build:catalog:apk:prod": "yarn run gradlew:clean && ENVFILE=.env.prod yarn run gradlew:apk:catalog --parallel",
		"build:catalog:apk:stage": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run gradlew:apk:catalog --parallel",
		"build:catalog:apk:dev": "yarn run gradlew:clean && ENVFILE=.env yarn run gradlew:apk:catalog --parallel",
		"debug:catalog": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run android --mode=catalogDebug",
		"debug:pos": "yarn run gradlew:clean && ENVFILE=.env.stage yarn run android --mode=posDebug",
		"clean:ios": "rm -rf package-lock.json && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && npm cache clean --force && npm cache verify && rm -rf ios/build && rm -rf node_modules/ && yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && cd ios && pod install && cd .. && rm -rf ~/Library/Developer/Xcode/DerivedData",
		"clean:android": "cd android && ./gradlew clean && cd .. && npx react-native start --reset-cache",
		"clean:linux": "rm -rf package-lock.json && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && npm cache clean --force && npm cache verify && rm -rf node_modules/ && yarn install && yarn run gradlew:clean && cd .. && npx react-native start --reset-cache",
		"clean:caches": "rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && npm cache clean --force && npm cache verify",
		"podinstall:legacy": "cd ios && rm -rf Pods/ && rm -rf Podfile.lock && arch -x86_64 pod install --repo-update",
		"start:legacy": "yarn start",
		"lint": "eslint src/ --max-warnings=7",
		"lint:fix": "eslint src/ --fix",
		"prettier:fix": "prettier --write src/",
		"prettier:check": "prettier --check src/"
	},
